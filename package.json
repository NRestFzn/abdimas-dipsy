{
  "name": "express-sequelize",
  "version": "1.0.0",
  "description": "express-sequelize",
  "main": "src/main.js",
  "scripts": {
    "key:generate": "node ./src/scripts/GenerateJwtEnv.js",
    "start": "nodemon --exec babel-node src/main.js",
    "build": "yarn && npm-run-all tailwind:build start",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:refresh": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "yarn && npm-run-all db:drop db:create db:migrate db:seed",
    "tailwind:build": "tailwindcss build -i public/assets/css/tailwind.config.css -o public/assets/css/tailwind.css"
  },
  "author": "Nashir Resta",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.24.0",
    "autoprefixer": "^10.4.8",
    "axios": "^1.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "chalk": "^5.0.1",
    "colorette": "^2.0.20",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "google-auth-library": "^8.8.0",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sequelize": "^6.21.5",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "tailwindcss": "^3.1.8",
    "uuid": "^9.0.1",
    "yup": "^0.32.11"
  }
}
