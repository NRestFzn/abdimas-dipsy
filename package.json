{
  "name": "nabung-yaso",
  "version": "1.0.0",
  "description": "template express & nodejs for my self",
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon src/app.js",
    "build": "yarn && npm-run-all tailwind:build start",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:refresh": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "yarn && npm-run-all db:drop db:create db:migrate db:seed",
    "tailwind:build": "tailwindcss build -i public/assets/css/tailwind.config.css -o public/assets/css/tailwind.css"
  },
  "author": "liz",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.8",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sequelize": "^6.21.5",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "tailwindcss": "^3.1.8",
    "yup": "^0.32.11"
  }
}
